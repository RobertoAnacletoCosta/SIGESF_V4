@model List<SIGESF.ViewModels.RelatorioGeralAgentesSupridosViewModel>

@{
    ViewBag.Title = "Relatório Geral de Agentes Supridos";
}

<h2>@ViewBag.Title</h2>



<table class="table table-striped table-hover">
    <tr>
        <th>
            @Html.Display("Nome")
        </th>
        <th>
            @Html.Display("Matricula")
        </th>
        <th>
            @Html.Display("CPF")
        </th>
        <th>
            @Html.Display("UnidadeLotacao")
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CPF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnidadeLotacao)
            </td>
        </tr>
    }
</table>